{
  "build": {
    "core": "esp8266",
    "extra_flags": "-DLWIP_OPEN_SRC   -DARDUINO=10605  -DARDUINO_ARCH_esp8266  -DESP8266",
    "f_cpu": "80000000L",
    "f_flash": "80000000L",
    "flash_mode": "qio",
    "ldscript": "esp8266.flash.4m.ld",
    "mcu": "esp8266",
    "variant": "nodemcu"
  },
  "frameworks": [
    "arduino"
  ],
  "name": "Arduino UNO WiFi's ESP8266",
  "upload": {
    "maximum_ram_size": 81920,
    "maximum_size": 1044464,
    "require_upload_port": true,
    "resetmethod" : "nodemcu",
    "speed": 9600
  },
  "url": "http://www.arduino.org/products/boards/arduino-uno-wifi",
  "vendor": "Espressif"
}
